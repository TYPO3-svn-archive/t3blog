<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>

	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:t3blog/pi2/locallang.xml:pi_flexform.defaultTab</sheetTitle>
				</TCEforms>
				<type>array</type>

				<el>
					<widget>
						<TCEforms>
							<label>LLL:EXT:t3blog/pi2/locallang.xml:pi_flexform.select_widget</label>
							<config>
								<type>select</type>
								<size>12</size>
								<maxitems>1</maxitems>
								<items type="array"></items>
								<itemsProcFunc>tx_t3blog_pi2_addFieldsToFlexForm->getWidgets</itemsProcFunc>
							</config>
						</TCEforms>
					</widget>

					<storagePid>
						<TCEforms>
							<label>LLL:EXT:t3blog/pi2/locallang.xml:pi_flexform.storage_pid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
							</config>
						</TCEforms>
					</storagePid>
				</el>

			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>